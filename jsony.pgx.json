{
   "+grammar" : "jsony",
   "+toprule" : "jsony",
   "+version" : "0.0.1",
   "EOL" : {
      ".rgx" : "\\r?\\n"
   },
   "LCURLY" : {
      ".rgx" : "\\{"
   },
   "LSQUARE" : {
      ".rgx" : "\\["
   },
   "RCURLY" : {
      ".rgx" : "\\}"
   },
   "RSQUARE" : {
      ".rgx" : "\\]"
   },
   "bare" : {
      ".rgx" : "([^\\s\\{\\}\\[\\]'\",]*[^\\s\\{\\}\\[\\]'\",:])"
   },
   "comment" : {
      ".rgx" : "(?:\\#\\ .*\\r?\\n|\\#\\r?\\n|\\ *\\r?\\n)"
   },
   "double" : {
      ".rgx" : "\"((?:\\\\(?:[\"\\\\/bfnrt]|u[0-9a-fA-F]{4})|[^\"\\x00-\\x1f])*)\""
   },
   "jsony" : {
      ".any" : [
         {
            ".ref" : "seq"
         },
         {
            ".ref" : "map"
         },
         {
            ".ref" : "top_seq"
         },
         {
            ".ref" : "top_map"
         },
         {
            ".ref" : "list"
         }
      ]
   },
   "list" : {
      "+min" : 0,
      ".ref" : "node",
      ".sep" : {
         "+eok" : 1,
         ".rgx" : "(?:\\s|(?:\\#\\ .*\\r?\\n|\\#\\r?\\n|\\ *\\r?\\n))*,?(?:\\s|(?:\\#\\ .*\\r?\\n|\\#\\r?\\n|\\ *\\r?\\n))*"
      }
   },
   "map" : {
      ".all" : [
         {
            ".rgx" : "(?:\\s|(?:\\#\\ .*\\r?\\n|\\#\\r?\\n|\\ *\\r?\\n))*"
         },
         {
            ".ref" : "LCURLY"
         },
         {
            ".rgx" : "(?:\\s|(?:\\#\\ .*\\r?\\n|\\#\\r?\\n|\\ *\\r?\\n))*"
         },
         {
            "+min" : 0,
            ".ref" : "pair"
         },
         {
            ".rgx" : "(?:\\s|(?:\\#\\ .*\\r?\\n|\\#\\r?\\n|\\ *\\r?\\n))*"
         },
         {
            ".ref" : "RCURLY"
         },
         {
            ".rgx" : "(?:\\s|(?:\\#\\ .*\\r?\\n|\\#\\r?\\n|\\ *\\r?\\n))*"
         }
      ]
   },
   "node" : {
      ".any" : [
         {
            ".ref" : "map"
         },
         {
            ".ref" : "seq"
         },
         {
            ".ref" : "scalar"
         }
      ]
   },
   "pair" : {
      ".all" : [
         {
            ".ref" : "string"
         },
         {
            ".rgx" : "(?:\\s|(?:\\#\\ .*\\r?\\n|\\#\\r?\\n|\\ *\\r?\\n))*:?(?:\\s|(?:\\#\\ .*\\r?\\n|\\#\\r?\\n|\\ *\\r?\\n))*"
         },
         {
            ".ref" : "node"
         },
         {
            ".rgx" : "(?:\\s|(?:\\#\\ .*\\r?\\n|\\#\\r?\\n|\\ *\\r?\\n))*,?(?:\\s|(?:\\#\\ .*\\r?\\n|\\#\\r?\\n|\\ *\\r?\\n))*"
         }
      ]
   },
   "scalar" : {
      ".any" : [
         {
            ".ref" : "double"
         },
         {
            ".ref" : "single"
         },
         {
            ".ref" : "bare"
         }
      ]
   },
   "seq" : {
      ".all" : [
         {
            ".rgx" : "(?:\\s|(?:\\#\\ .*\\r?\\n|\\#\\r?\\n|\\ *\\r?\\n))*"
         },
         {
            ".ref" : "LSQUARE"
         },
         {
            ".rgx" : "(?:\\s|(?:\\#\\ .*\\r?\\n|\\#\\r?\\n|\\ *\\r?\\n))*"
         },
         {
            "+min" : 0,
            ".ref" : "node",
            ".sep" : {
               "+eok" : 1,
               ".rgx" : "(?:\\s|(?:\\#\\ .*\\r?\\n|\\#\\r?\\n|\\ *\\r?\\n))*,?(?:\\s|(?:\\#\\ .*\\r?\\n|\\#\\r?\\n|\\ *\\r?\\n))*"
            }
         },
         {
            ".rgx" : "(?:\\s|(?:\\#\\ .*\\r?\\n|\\#\\r?\\n|\\ *\\r?\\n))*"
         },
         {
            ".ref" : "RSQUARE"
         },
         {
            ".rgx" : "(?:\\s|(?:\\#\\ .*\\r?\\n|\\#\\r?\\n|\\ *\\r?\\n))*"
         }
      ]
   },
   "single" : {
      ".rgx" : "'([^']*)'"
   },
   "string" : {
      ".ref" : "scalar"
   },
   "top_map" : {
      "+min" : 1,
      ".all" : [
         {
            ".ref" : "string"
         },
         {
            ".rgx" : "(?:\\s|(?:\\#\\ .*\\r?\\n|\\#\\r?\\n|\\ *\\r?\\n))*:(?:\\s|(?:\\#\\ .*\\r?\\n|\\#\\r?\\n|\\ *\\r?\\n))*"
         },
         {
            ".ref" : "node"
         },
         {
            ".rgx" : "(?:\\s|(?:\\#\\ .*\\r?\\n|\\#\\r?\\n|\\ *\\r?\\n))*"
         }
      ]
   },
   "top_seq" : {
      "+min" : 1,
      ".ref" : "top_seq_entry"
   },
   "top_seq_entry" : {
      ".all" : [
         {
            ".rgx" : "(?:\\s|(?:\\#\\ .*\\r?\\n|\\#\\r?\\n|\\ *\\r?\\n))*\\-\\ +"
         },
         {
            ".all" : [
               {
                  "+min" : 0,
                  ".ref" : "node",
                  ".sep" : {
                     "+eok" : 1,
                     ".rgx" : "(?:\\ *,\\ *\\r?\\n(?:\\s|(?:\\#\\ .*\\r?\\n|\\#\\r?\\n|\\ *\\r?\\n))*|\\ +)"
                  }
               },
               {
                  ".any" : [
                     {
                        ".ref" : "comment"
                     },
                     {
                        ".ref" : "EOL"
                     }
                  ]
               }
            ]
         }
      ]
   }
}
